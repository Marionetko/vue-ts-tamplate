{
  "name": "vue-ts-tamplate",
  "description": "Vue.js template with TypeScript",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "local-server": "vite",
    "ts-check-and-build": "run-p type-check \"build-only {@}\" --",
    "build-preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "axios": "1.12.2",
    "vue": "3.5.22",
    "vue-i18n": "11.1.12",
    "vue-router": "4.6.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.2",
    "@types/node": "24.7.2",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.8.1",
    "npm-run-all2": "8.0.4",
    "typescript": "5.9.3",
    "vite": "7.1.10",
    "vite-plugin-vue-devtools": "8.0.2",
    "vue-tsc": "3.1.1"
  }
}
